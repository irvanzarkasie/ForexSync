                <xt:location>0</xt:location>
                <xt:value xsi:type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema">mailto:bi.soa@infracom-tech.com</xt:value>
            </xt:replace>
            <xt:replace>
