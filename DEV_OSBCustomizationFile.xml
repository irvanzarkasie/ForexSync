                        <ema:from-name>SOA</ema:from-name>
                        <ema:reply-to-address/>
                        <ema:reply-to-name/>
                        <ema:connection-timeout>0</ema:connection-timeout>
                        <ema:request-encoding>iso-8859-1</ema:request-encoding>
                    </ema:email-destination-entry>
                </xt:value>
            </xt:replace>
        </cus:actions>
    </cus:customization>
